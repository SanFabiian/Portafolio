*
	margin 0
	padding 0
	text-decoration none
	list-style none
	font-family arial
	box-sizing border-box
	font-size 100px
.btn
	font-size font_size-subtitle
	padding size_padding
	border none
	&:active
		translate .5s
		transform scale(.95)
.btn-primary
	background color_third
	color color_light
	width 50%
	justify-self center
